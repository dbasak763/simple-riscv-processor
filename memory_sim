    %delay 1000, 0;
    %load/vec4 v0x7f8b918258a0_0;
    %load/vec4 v0x7f8b918242b0_0;
    %cmp/e;
    %flag_get/vec4 6;
    %jmp/0 T_0.2, 6;
    %load/vec4 v0x7f8b91825930_0;
    %pushi/vec4 1, 0, 1;
    %cmp/e;
    %flag_get/vec4 6;
    %and;
T_0.2;
